#include<bits/stdc++.h>

using namespace std;


int main(){
    string in;
    cin >> in;
    for(int i = 0; i < in.size();){
        if(in.substr(i, 2) == "pi" || in.substr(i, 2) =="ka")
            i += 2;
        else if(in.substr(i, 3) == "chu")
            i += 3;
        else{
            cout << "NO";
            return 0;
        }
    }
    cout << "YES";
}
