#include <string>
#include <iostream>
#include <cmath>
#include <queue>
using namespace std;
#define MAX 987654321
#define endl "\n"
int i, j, x, y, k, N, M, T, K, W, V;


int main(){
    cin >> N;
    string s = "";
    int temp = N;
    while(temp--)
        s += "*";
    temp = N;
    while(temp--)
        cout << s << endl;
}
