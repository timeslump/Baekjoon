#include<iostream>
#include<vector>
#include<stack>
#include<algorithm>

#define endl "\n"
typedef long long ll;
using namespace std;


int main(){
    int cnt = 0;
    int N;
    cin >> N;
    string s;
    cin >> s;
    for(int i = 0; i < s.size(); i++)
        cnt += s[i] - '0';
    cout << cnt;
}
