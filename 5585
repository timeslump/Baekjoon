#include<iostream>
#include<algorithm>
#include <vector>

using namespace std;
int main(){
    int money;
    cin >> money;
    int cnt = 0;
    money = 1000 - money;
    cnt = money / 500 + cnt;
    money = money % 500;
    cnt = money / 100 + cnt;
    money = money % 100;
    cnt = money / 50 + cnt;
    money = money % 50;
    cnt = money / 10 + cnt;
    money = money % 10;
    cnt = money / 5 + cnt;
    money = money % 5;
    cnt = money / 1 + cnt;
    cout << cnt << endl;
}
