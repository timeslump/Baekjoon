#include<iostream>
#include<bitset>
#include<queue>
using namespace std;

int Map[8][8];
int dx[4]= {0, 1, 0 , -1};
int dy[4] = {-1, 0, 1, 0};

int N, M;
int main(){
    cin >> N;
    for(int i = 1; i <= N; i++){
        int tmp = i;
        while(tmp--)
            cout << "*";
        cout << endl;
    }
    for(int i = N -1; i > 0; i--){
        int tmp = i;
        while(tmp--)
            cout << "*";
        cout << endl;
    }
}
